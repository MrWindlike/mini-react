{
  "name": "mini-react",
  "version": "1.0.0",
  "description": "This project is the mini version of React.",
  "scripts": {
    "test": "lerna run test --stream",
    "install": "lerna bootstrap",
    "build": "lerna run build",
    "dev": "lerna run dev",
    "precommit": "lerna run precommit --stream",
    "commit": "git cz",
    "lint": "lerna run lint",
    "version": "conventional-changelog -i CHANGELOG.md -s -p conventional-changelog-emoji && git add CHANGELOG.md",
    "log": "conventional-changelog -i CHANGELOG.md -s -r 0 -p conventional-changelog-emoji"
  },
  "keywords": [],
  "author": "MrWindlike",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@tsconfig/recommended": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "commitizen": "^4.0.4",
    "conventional-changelog": "^3.1.18",
    "conventional-changelog-cli": "^2.0.31",
    "conventional-changelog-emoji": "^0.3.3",
    "cz-customizable": "^6.2.0",
    "error": "^10.4.0",
    "eslint": "^8.7.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^4.2.5",
    "lerna": "^4.0.0",
    "lint-staged": "^12.2.2",
    "typescript": "^4.5.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
